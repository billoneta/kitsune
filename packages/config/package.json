  "devDependencies": {
    "@commitlint/types": "^19.8.1",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/node": "22.15.29",
    "glob": "^11.0.2",
    "rimraf": "6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "eslint": "9.28.0",
    "prettier": "3.5.3"
  },
